<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OpenAINetMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OpenAINetMaui"
    xmlns:views="clr-namespace:OpenAINetMaui.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Chat"
        ContentTemplate="{DataTemplate views:ChatView}"
        Route="ChatPage" />

    <ShellContent
        Title="Image"
        ContentTemplate="{DataTemplate views:ImageView}"
        Route="ImagePage" />
</Shell>
